{
	"name": "cli",
	"version": "0.0.0",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch"
	},
	"files": ["dist"],
	"dependencies": {
		"ink": "^4.1.0",
		"ink-spinner": "^5.0.0",
		"meow": "^11.0.0",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"ink-testing-library": "^3.0.0",
		"ts-node": "^10.9.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": ["--loader=ts-node/esm"]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	}
}
