---
export const prerender = false;

import Layout from '@/layouts/Layout.astro';
import { DashboardPageWrapper } from '../../../components/Dashboard/DashboardPageWrapper.tsx';
import { KickChatPage } from '../../../components/Dashboard/KickChatPage.tsx';

const { slug } = Astro.params;
---

<Layout>
	<DashboardPageWrapper client:only="react" slug={slug} pageTitle="Kick Chat">
		{(props: any) => <KickChatPage client:only="react" {...props} />}
	</DashboardPageWrapper>
</Layout>
