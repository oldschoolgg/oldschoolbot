---
export const prerender = false;

import { DashboardPageWrapper } from '../../../components/Dashboard/DashboardPageWrapper.tsx';
import { TimePage } from '../../../components/Dashboard/TimePage.tsx';
import Layout from '../../../layouts/Layout.astro';

const { slug } = Astro.params;
---

<Layout>
	<DashboardPageWrapper client:only="react" slug={slug} pageTitle="Time">
		{(props: any) => <TimePage client:only="react" {...props} />}
	</DashboardPageWrapper>
</Layout>
