---
export const prerender = false;

import { DashboardPageWrapper } from '../../../components/Dashboard/DashboardPageWrapper.tsx';
import { DiscordHighlightsPage } from '../../../components/Dashboard/DiscordHighlights.tsx';
import Layout from '../../../layouts/Layout.astro';

const { slug } = Astro.params;
---

<Layout>
	<DashboardPageWrapper
		client:only="react"
		slug={slug}
		pageTitle="Discord Highlights"
	>
		{
			(props: any) => (
				<DiscordHighlightsPage client:only="react" {...props} />
			)
		}
	</DashboardPageWrapper>
</Layout>
