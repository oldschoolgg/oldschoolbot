{
	"name": "oldschooljs",
	"version": "0.0.1",
	"private": true,
	"description": "Allows you to interact with the OSRS Hiscores, Wiki, Items, & more.",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/esm/index.d.mts",
			"require": "./dist/cjs/index.cjs",
			"import": "./dist/esm/index.mjs"
		},
		"./*": {
			"types": "./dist/esm/index.d.mts",
			"require": "./dist/cjs/index.cjs",
			"import": "./dist/esm/index.mjs"
		}
	},
	"license": "MIT",
	"scripts": {
		"test": "concurrently \"pnpm test:unit\" \"tsc --noEmit -p test\" \"tsc --noEmit -p src\"",
		"test:unit": "vitest run --coverage",
		"generate": "tsx --tsconfig scripts/tsconfig.json scripts/prepare.ts",
		"scrapeitems": "tsx --tsconfig scripts/tsconfig.json scripts/item-scraper.ts",
		"moid": "tsx --tsconfig scripts/tsconfig.json scripts/scraper/moid.ts",
		"prepareitems": "tsx --tsconfig scripts/tsconfig.json scripts/prepareItems.ts && tsx --tsconfig scripts/tsconfig.json scripts/itemsPostProcess.ts",
		"enum": "tsx --tsconfig scripts/tsconfig.json scripts/enum.ts",
		"build": "tsdown"
	},
	"dependencies": {
		"deepmerge": "4.3.1"
	},
	"devDependencies": {
		"deep-object-diff": "^1.1.9",
		"wtf_wikipedia": "^10.3.2"
	}
}