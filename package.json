		"@prisma/client": "^3.15.1",
		"@sapphire/stopwatch": "^1.5.0",
		"@sapphire/time-utilities": "^1.7.8",
		"@sentry/node": "^7.38.0",
		"bufferutil": "^4.0.7",
		"chart.js": "^3.9.1",
		"chartjs-node-canvas": "github:gc/ChartjsNodeCanvas#a598b6dd27c44351f235bca07ca4ee660121f289",
		"chartjs-plugin-datalabels": "^2.2.0",
		"chokidar": "^3.5.3",
		"discord.js": "^14.7.1",
		"e": "^0.2.32",
		"emoji-regex": "^9.2.2",
		"fastify": "^3.29.4",
		"fastify-cors": "^5.2.0",
		"fastify-helmet": "^5.3.2",
		"fastify-rate-limit": "^5.9.0",
		"fastify-raw-body": "^3.2.0",
		"fastify-sensible": "^3.2.0",
		"he": "^1.2.0",
		"lru-cache": "^7.17.0",
		"mahoji": "^0.0.62",
		"math-expression-evaluator": "^1.4.0",
