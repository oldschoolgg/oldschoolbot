		"piscina": "^3.2.0",
		"random-js": "^2.1.0",
		"table": "^6.8.1",
		"utf-8-validate": "^5.0.10",
		"zlib-sync": "^0.1.8"
	},
	"devDependencies": {
		"@oldschoolgg/eslint-config": "^2.0.13",
		"@oldschoolgg/ts-config": "^0.0.1",
		"@sinclair/typebox": "^0.12.9",
		"@types/he": "^1.2.0",
		"@types/madge": "^5.0.0",
		"@types/math-expression-evaluator": "^1.3.0",
		"@types/node": "^14.18.36",
		"@types/node-cron": "^3.0.7",
		"@types/node-fetch": "^2.6.2",
