		"node-cron": "^3.0.2",
		"node-fetch": "^2.6.7",
		"oldschooljs": "^2.2.3",
		"p-queue": "^6.6.2",
		"pino": "^8.11.0",
		"piscina": "^3.2.0",
		"random-js": "^2.1.0",
		"table": "^6.8.0",
		"utf-8-validate": "^6.0.3",
		"zlib-sync": "^0.1.8",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@oldschoolgg/eslint-config": "^2.0.13",
		"@oldschoolgg/ts-config": "^0.0.1",
		"@types/he": "^1.1.2",
