		"@types/node-cron": "^3.0.7",
		"@types/node-fetch": "^2.6.1",
		"@typescript-eslint/eslint-plugin": "^5.41.0",
		"@typescript-eslint/parser": "^5.41.0",
		"@vitest/coverage-v8": "^1.3.1",
		"concurrently": "^7.6.0",
