  @@map("guilds")
}

model NewUser {
  id            String @id @db.VarChar(19)
  pizazz_points Int    @default(0)

  // Relations
  minigame_id  Int?         @unique
  minigame     Minigame?    @relation(fields: [minigame_id], references: [id])
