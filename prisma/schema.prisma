enum GearSetupType {
  melee
  range
  mage
  misc
  skilling
  wildy
  fashion
  other
}

model GearPreset {
  user_id    String @db.VarChar(19)
  name       String @db.VarChar(24)
  two_handed Int?
  body       Int?
  cape       Int?
  feet       Int?
  hands      Int?
  head       Int?
  legs       Int?
  neck       Int?
  ring       Int?
  shield     Int?
  weapon     Int?
