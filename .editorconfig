# EditorConfig is awesome: https://EditorConfig.org

# Mark file as the root so that no more parent directory searches are performed
root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{js,ts,.astro,.md,.tsx,.jsx}]
charset = utf-8
indent_style = tab
indent_size = 4
